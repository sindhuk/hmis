<h3>Editing ward Master</h3>
<br/>
<br/>
<% form_for(@wardmaster) do |f| %>
  <%= f.error_messages %>
<div align="center" width="100%">
  <table width="20%"  cellspacing="0" border="1" cellpadding="4">
<tr><td>
    <%= f.label :floormaster_id %></td>
   <td> <%= f.text_field :floormaster_id %>
  </td>  <tr/>
 <tr><td>
    <%= f.label :name %></td>
   <td> <%= f.text_field :name %>
  </td>  <tr/>
  <tr><td>
    <%= f.label :code %></td>
   <td> <%= f.text_field :code %>
  </td>  <tr/>
  <tr>	<td>
    <%= f.label :cost %></td>
   <td> <%= f.text_field :cost %>
  </td>  <tr/>
  <tr><td>
    <%= f.label :status %></td>
   <td> <%= f.text_field :status %>
 </td>  <tr/>
  <tr>	<td>
    <%= f.label :incharge %></td>
   <td> <%= f.select :incharge, Employeemaster.find(:all).collect {|p| [p.name]} %>
  </td>  <tr/><table/>
  <%= f.submit 'Update' %>
<% end %>

