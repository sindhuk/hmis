<h1>New usgtable</h1>

<script type="text/javascript">
    function check_val()
	{
		len=document.getElementById('reg').value
		if(len.length==5)
		{
			window.location='http://localhost:3000/usgtables/new?reg='+len
		}	
	}
</script>

