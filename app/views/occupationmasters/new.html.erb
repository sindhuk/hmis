<h3>New Occupationmaster</h3>
<br/><br/>
<div align="center" width="90%">
<table width="40%" style="border:#966 solid 1px;border-color:#FFF0; " border="1" cellspacing="0" cellpadding="4">

<% form_for(@occupationmaster) do |f| %>
  <%= f.error_messages %>

  <tr><td>
    <%= f.label :name %></td>
    <td><%= f.text_field :name %></td></tr>
  <tr><td>
    <%= f.label :code %></td>
    <td><%= f.text_field :code %> </td></tr>
  </table>
    <tr><td><%= f.submit 'Create' %></td></tr>
  
<% end %>
</div>
<tr><td><%= link_to 'show', occupationmasters_path %></td></tr></table>