<h1>Listing patientappointmentinfos</h1>

<table>
  <tr>
    <th>Patientregistration</th>
   
    <th>Regdatedate</th>
    <th>Registrationid</th>
    <th>RefferedbyDoctor</th>
    <th>Timepfappointment</th>
  </tr>

<% @patientappointmentinfos.each do |patientappointmentinfo| %>
  <tr>
    <td><%=h patientappointmentinfo.patient_registration_id %></td>
    
    <td><%=h patientappointmentinfo.reg_date %></td>
    <td><%=h patientappointmentinfo.registration_id %></td>
    <td><%=h patientappointmentinfo.reffered_by_doctor %></td>
    <td><%=h patientappointmentinfo.time_of_appointment %></td>
    <td><%= link_to 'Show', patientappointmentinfo %></td>
    <td><%= link_to 'Edit', edit_patientappointmentinfo_path(patientappointmentinfo) %></td>
    <td><%= link_to 'Destroy', patientappointmentinfo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New patientappointmentinfo', new_patientappointmentinfo_path %>