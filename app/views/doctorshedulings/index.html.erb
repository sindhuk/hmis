<br><br>
<div align="center">
<table>
  <tr>
    <th>Department name</th>
    <th>Doctor</th>
    <th>Doctor name</th>
    <th>Date</th>
	<th>Show</th>
	 <th>Edit</th>
	 <th>Destroy</th>
  </tr>

<% @doctorshedulings.each do |doctorsheduling| %>
 <% if((doctorsheduling.id % 2)==0) 
		id="datacellone"
	else
		id="datacelltwo"
  end %>
 <tr>
    <td class='<%= id %>'><%=h doctorsheduling.department_name %></td>
    <td class='<%= id %>'><%=h doctorsheduling.doctor_id %></td>
    <td class='<%= id %>'><%=h doctorsheduling.doctor_name %></td>
    <td class='<%= id %>'><%=h doctorsheduling.date %></td>
    <td class='<%= id %>'><%= link_to 'Show', doctorsheduling %></td>
    <td class='<%= id %>'><%= link_to 'Edit', edit_doctorsheduling_path(doctorsheduling) %></td>
    <td class='<%= id %>'><%= link_to 'Destroy', doctorsheduling, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

</div>
<br>
