<h3> Editing Test Master</h3>
<br/>
<br/>
<% form_for(@testmaster) do |f| %>
  <%= f.error_messages %>
<div align="center">
  <table width="20%"  cellspacing="0" border="1" cellpadding="4">
		<tr>
		<td>Code </td>
    <td><%= f.text_field :test_code %></td>
  <tr>
 <td>Name </td>
    <td><%= f.text_field :test_name %></td>
  </tr>
  <tr>
    <td>Fee</td>
    <td><%= f.text_field :fee %></td>
</tr>
  </table>
  <br/>
  <%= f.submit 'Update' %>
  </div>
  
<%= link_to 'Back', testmaster_path %>